{
  "name": "babylon-scriptorium",
  "version": "1.0.0",
  "description": "A garden of forking agents â€” multi-agent LLM orchestration for coding tasks",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "babylon": "dist/main.js"
  },
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "llm",
    "agents",
    "orchestration",
    "multi-agent",
    "typescript",
    "babylon-scriptorium"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.18.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.11.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.4.2",
    "semantic-release": "^24.2.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.21.0",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "debug": "^4.4.3",
    "execa": "^9.6.1",
    "log-update": "^7.1.0",
    "openai": "^6.19.0"
  }
}
